    "@typescript-eslint/eslint-plugin": "~5.25.0",
    "@typescript-eslint/parser": "~5.18.0",
    "cypress": "^9.7.0",
    "eslint": "~8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.2",
