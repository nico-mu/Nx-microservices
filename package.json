{
  "name": "nx-microservices",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "setup": "yarn install && nx run models:gen-client",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx affected:build --base=main --configuration=production",
    "test": "npx nx affected --target=test --parallel=3 --code-coverage --base main",
    "dump-postgres": "./scripts/database/dump.sh",
    "migrate": "prisma migrate dev --schema ./libs/models/src/prisma/schema.prisma",
    "serve": "docker compose -f docker-compose.yml up -d && nx serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "@nestjs/microservices": "^8.4.5",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/swagger": "^5.2.1",
    "@nrwl/angular": "14.1.7",
    "@prisma/client": "^3.14.0",
    "bcrypt": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.5",
    "swagger-ui-express": "^4.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.7",
    "@angular-eslint/eslint-plugin": "~13.2.1",
    "@angular-eslint/eslint-plugin-template": "~13.2.1",
    "@angular-eslint/template-parser": "~13.2.1",
    "@angular/cli": "~13.3.6",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.9",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@nrwl/cli": "14.1.7",
    "@nrwl/cypress": "14.1.7",
    "@nrwl/devkit": "14.1.7",
    "@nrwl/eslint-plugin-nx": "14.1.7",
    "@nrwl/jest": "14.1.9",
